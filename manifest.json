{
  "manifest_version": 3,
  "name": "Tensor Ad Detector",
  "version": "1.1",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://*.wildberries.ru/*"
      ],
      "js": [
        "tf-core.min.js", 
        "tf-backend-cpu.min.js", 
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
